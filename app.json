{
  "expo": {
    "name": "THEFOURTHBOOK",
    "slug": "THEFOURTHBOOK",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "thefourthbook",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleLocalizations": [
          "en",
          "fr"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shopwit.THEFOURTHBOOK",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "thefourthbook",
              "host": "payments",
              "pathPrefix": "/callback"
            }
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": [
              "en",
              "fr"
            ],
            "android": [
              "en",
              "fr"
            ]
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow THEFOURTHBOOK to access your photos so you can upload your ID front image.",
          "cameraPermission": "Allow THEFOURTHBOOK to use the camera to capture your ID and selfie."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd4d0b9b-24bb-4cda-8981-59b7a7d37417"
      }
    }
  }
}
